{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@common/*": [
        "src/common/*"
      ],
      "@middleware/*": [
        "src/common/middleware/*"
      ],
      "@filter/*": [
        "src/common/filter/*"
      ],
      "@config/*": [
        "src/common/configuration/*"
      ],
      "@decorator/*": [
        "src/common/decorator/*"
      ],
      "@exception/*": [
        "src/common/exception/*"
      ],
      "@domain/*": [
        "src/domain/*"
      ]
    }
  }
}